@model Toolivahendus2.Models.TooliVahendus

@{
    ViewData["Title"] = "Andmete Kustutamine";
}

<h1>Andmete Kustutamine</h1>

<h3>Peale kustutamist ei ole võimalik andmeid taastada.</h3>
<div>

    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Eesnimi)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Eesnimi)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Perekonnanimi)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Perekonnanimi)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Firmanimi)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Firmanimi)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Firmaemail)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Firmaemail)
        </dd>
    </dl>


    <form asp-action="Kustutamine">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Eemalda" class="btn btn-danger" />
    </form>
    <hr />
</div>
